{
    "name": "jarhen/codifire-menu",
    "description": "drag and drop menu generator like wordpress for laravel 5",
    "keywords": ["Laravel", "Menu", "Wordpress", "Builder"],
    "authors": [
    	{
            "name": "Ramos Jarhen",
            "email": "jarhen@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.*"
    },
    "autoload": {
        "psr-4": {
          "Jarhen\\Menu\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jarhen\\Menu\\MenuServiceProvider"
            ],
            "aliases": {
                "Menu": "Jarhen\\Menu\\Facades\\Menu"
            }
        }
    }
}
